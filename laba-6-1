#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;
    if (N <= 0) return 0;
    double *A = new double[N];
    for (int i = 0; i < N; i++) {
        cin >> A[i];
    }
    double sum = 0;
    for (int i = 0; i < N; i++) {
        sum += A[i];
    }
    double avg = sum / N;
    cout << avg;
    delete[] A;
    return 0;
}
