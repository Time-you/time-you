#include <iostream>
using namespace std;

class VectorFloat {
    float* a;
    int size;
    int codeError;
    static int countObj;
public:
    VectorFloat() {
        size = 1;
        a = new float[1];
        a[0] = 0;
        codeError = 0;
        countObj++;
    }

    VectorFloat(int n) {
        size = n;
        a = new float[n];
        for(int i=0;i<n;i++) a[i]=0;
        codeError = 0;
        countObj++;
    }

    VectorFloat(int n, float val) {
        size = n;
        a = new float[n];
        for(int i=0;i<n;i++) a[i]=val;
        codeError = 0;
        countObj++;
    }

    ~VectorFloat() {
        delete[] a;
    }

    VectorFloat& operator=(const VectorFloat& v) {
        if(this==&v) return *this;
        delete[] a;
        size = v.size;
        a = new float[size];
        for(int i=0;i<size;i++) a[i] = v.a[i];
        return *this;
    }

    float& operator[](int i) {
        if(i<0 || i>=size) {
            codeError = 1;
            return a[size-1];
        }
        return a[i];
    }

    VectorFloat operator+(const VectorFloat& v) {
        VectorFloat t(size);
        for(int i=0;i<size;i++) t.a[i]=a[i]+v.a[i];
        return t;
    }

    VectorFloat operator-(const VectorFloat& v) {
        VectorFloat t(size);
        for(int i=0;i<size;i++) t.a[i]=a[i]-v.a[i];
        return t;
    }

    VectorFloat operator*(float x) {
        VectorFloat t(size);
        for(int i=0;i<size;i++) t.a[i]=a[i]*x;
        return t;
    }

    friend ostream& operator<<(ostream& out, const VectorFloat& v) {
        for(int i=0;i<v.size;i++) out<<v.a[i]<<" ";
        return out;
    }

    friend istream& operator>>(istream& in, VectorFloat& v) {
        for(int i=0;i<v.size;i++) in>>v.a[i];
        return in;
    }
};

int VectorFloat::countObj = 0;

int main() {
    int n;
    cin >> n;
    VectorFloat a(n), b(n);

    cin >> a >> b;

    VectorFloat c = (a + b) * 2 - b + a * 3;

    cout << c;
    return 0;
}
