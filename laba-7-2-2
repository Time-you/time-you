#include <iostream>
#include <fstream>
using namespace std;

int main() {
    ifstream f("out.bin",ios::binary);
    unsigned short x;

    char out[4][16];

    for(int i=0;i<4;i++)
        for(int j=0;j<16;j++)
            out[i][j]=' ';

    for(int i=0;i<4*16;i++){
        f.read((char*)&x,2);

        int row = x & 3;
        int high = (x>>2)&15;
        int low = (x>>7)&15;
        int pos = (x>>11)&15;

        char c = (high<<4) | low;
        out[row][pos] = c;
    }

    f.close();

    for(int r=0;r<4;r++){
        for(int j=0;j<16;j++)
            cout<<out[r][j];
        cout<<"\n";
    }
}
