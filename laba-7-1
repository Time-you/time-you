#include <iostream>
using namespace std;

int main() {
    int a,b,c,d;
    cin>>a>>b>>c>>d;

    int m1 = (c<<13) + (c<<7) + (c<<5) + (c<<3) + (c<<2) + c;
    int m2 = (d<<3) + (d<<2) + (d<<1);
    int part1 = (m1 - m2) >> 10;

    int m3 = (d<<5) + (d<<1);
    int part2 = (m3 + a) / b;

    int x = part1 + part2;
    cout<<x;
}
