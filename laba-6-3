#include <iostream>
using namespace std;

int main() {
    int n, m;
    cin >> n >> m;
    if (n <= 0 || m <= 0) return 0;

    double **A = new double*[n];
    for (int i = 0; i < n; i++) {
        A[i] = new double[m];
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cin >> A[i][j];
        }
    }

    double V;
    bool firstRow = true;

    for (int i = 0; i < n; i++) {
        double rowMax = A[i][0];
        for (int j = 1; j < m; j++) {
            if (A[i][j] > rowMax) {
                rowMax = A[i][j];
            }
        }
        if (firstRow) {
            V = rowMax;
            firstRow = false;
        } else {
            if (rowMax < V) {
                V = rowMax;
            }
        }
    }

    cout << V;

    for (int i = 0; i < n; i++) {
        delete[] A[i];
    }
    delete[] A;

    return 0;
}
