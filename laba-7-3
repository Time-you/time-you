#include <iostream>
#include <fstream>
#include <string>
using namespace std;

struct Bits {
    unsigned row:2;
    unsigned high:4;
    unsigned p1:1;
    unsigned low:4;
    unsigned pos:4;
    unsigned p2:1;
};

union U {
    Bits b;
    unsigned short v;
};

int main() {
    string s[4];
    for(int i=0;i<4;i++){
        getline(cin,s[i]);
        while(s[i].size()<16) s[i]+=' ';
    }

    ofstream f("out2.bin",ios::binary);

    for(int r=0;r<4;r++){
        for(int j=0;j<16;j++){
            U u;
            u.v = 0;
            char c = s[r][j];

            u.b.row = r;
            u.b.high = (c>>4)&15;

            int b=0; unsigned short t=1;
            unsigned short temp = (u.b.row) | (u.b.high<<2);
            for(int k=0;k<6;k++){
                if(temp&t) b^=1;
                t<<=1;
            }
            u.b.p1 = b;

            u.b.low = c&15;
            u.b.pos = j;

            b=0; t=1;
            for(int k=0;k<15;k++){
                if(u.v&t) b^=1;
                t<<=1;
            }
            u.b.p2 = b;

            f.write((char*)&u.v,2);
        }
    }

    f.close();
}
