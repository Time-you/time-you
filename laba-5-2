#include <iostream>
using namespace std;

class B1 {
protected:
    int x;
public:
    B1(int v = 0) {
        x = v;
        cout << "B1 constructor\n";
    }
    ~B1() {
        cout << "B1 destructor\n";
    }
    void show() {
        cout << "B1: " << x << endl;
    }
};

class B2 {
protected:
    int y;
public:
    B2(int v = 0) {
        y = v;
        cout << "B2 constructor\n";
    }
    ~B2() {
        cout << "B2 destructor\n";
    }
    void show() {
        cout << "B2: " << y << endl;
    }
};

class D1 : public B1, private B2 {
protected:
    int d1;
public:
    D1(int v1 = 0, int v2 = 0, int v = 0) : B1(v1), B2(v2) {
        d1 = v;
        cout << "D1 constructor\n";
    }
    ~D1() {
        cout << "D1 destructor\n";
    }
    void show() {
        cout << "D1: " << d1 << endl;
        B1::show();
        B2::show();
    }
};

class D2 : private D1 {
protected:
    int d2;
public:
    D2(int v1 = 0, int v2 = 0, int v3 = 0, int v = 0) : D1(v1, v2, v3) {
        d2 = v;
        cout << "D2 constructor\n";
    }
    ~D2() {
        cout << "D2 destructor\n";
    }
    void show() {
        cout << "D2: " << d2 << endl;
        D1::show();
    }
};

class D3 : public D2 {
    int d3;
public:
    D3(int v1 = 0, int v2 = 0, int v3 = 0, int v4 = 0, int v = 0)
        : D2(v1, v2, v3, v4) {
        d3 = v;
        cout << "D3 constructor\n";
    }
    ~D3() {
        cout << "D3 destructor\n";
    }
    void show() {
        cout << "D3: " << d3 << endl;
        D2::show();
    }
};

int main() {
    cout << "Створення об'єкта D3\n";
    D3 obj(1, 2, 3, 4, 5);
    cout << "\nВиведення значень:\n";
    obj.show();
    cout << "\nКiнець main\n";
    return 0;
}
